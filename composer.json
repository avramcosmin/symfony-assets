{
    "name": "mindlahus/symfony-assets",
    "description": "A list of PHP classes that will help you develop fast & easy various API's.",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "avramcosmin/forceutf8",
                "version": "2.0.1",
                "source": {
                    "url": "https://github.com/avramcosmin/forceutf8",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0": {
                      "ForceUTF8\\": "src/"
                    }
                }
          }
        }
    ],
    "require": {
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "symfony/symfony": "^3.3",
        "php": "^7.1",
        "symfony/monolog-bundle": "^3.1",
        "erusev/parsedown-extra": "^0.7",
        "friendsofsymfony/rest-bundle": "^2.2",
        "jms/serializer-bundle": "^2.0",
        "gedmo/doctrine-extensions": "^2.4",
        "stof/doctrine-extensions-bundle": "^1.2",
        "auth0/jwt-auth-bundle": "^3.0",
        "avramcosmin/forceutf8": "^2.0",
        "phpseclib/phpseclib": "^2.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.1",
        "symfony/phpunit-bridge": "^3.3",
        "mockery/mockery": "^0.9.9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Avram Cosmin",
            "email": "avram.cosmin@sprindo.com",
			"homepage": "http://www.sprindo.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mindlahus\\SymfonyAssets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Mindlahus\\SymfonyAssets\\": "tests/"
        }
    }
}
